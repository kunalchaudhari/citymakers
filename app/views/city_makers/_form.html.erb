<%= simple_form_for(@city_maker) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :avatar, as: :file %>
    <%= f.association :category, include_blank: false %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :lat %>
    <%= f.input :lng %>
    <%= f.input :address1 %>
    <%= f.input :address2 %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :country %>
    <%= f.input :phone %>
    <%= f.input :timings %>
    <%= f.input :note %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
